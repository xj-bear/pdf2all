{
  "name": "pdf2all-mcp",
  "version": "1.1.4",
  "description": "pdf2all全能转换器 - 一个强大的 MCP 服务器，支持将 PDF 转换为 Word、Excel (带 OCR)、PPT 和图片，并支持云端存储和 URL 处理。",
  "main": "dist/src/index.js",
  "bin": {
    "pdf2all-mcp": "dist/src/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && shx cp -r python dist/python",
    "start": "node dist/src/index.js",
    "start:sse": "node dist/src/index.js --sse",
    "dev": "tsc --watch",
    "postinstall": "pip3 install -r python/requirements.txt --break-system-packages || pip install -r python/requirements.txt || echo 'Warning: Python dependencies installation failed. Please install manually or use Docker.'"
  },
  "keywords": [
    "mcp",
    "pdf",
    "converter",
    "docx",
    "excel",
    "ppt",
    "jpg"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist",
    "python"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/uuid": "^10.0.0",
    "adm-zip": "^0.5.16",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "shx": "^0.4.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.13",
    "eventsource": "^4.1.0",
    "node-fetch": "^3.3.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}